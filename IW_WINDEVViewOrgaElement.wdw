     p_codes :
      -
        code : |1+
         // Procedure called for each drawing of an element found in the organization chart
         // <gOrgElement>: element to display 
         PROCEDURE IW_WINDEVViewOrgaElement( gOrgElement is an OrgElement )
         
         // Initialize the captions from the object to display
         STC_TitleOrgElement     = gOrgElement..Title
         STC_SubTitleOrgElement = gOrgElement..Content
         
         //if an image is found
         
         iImage is an Image = gOrgElement..Image
         IF iImage..Valid THEN
         	// make the image visible and assign it to the image control
         	IMG_ImageOrgElement..Visible = True
         	IMG_ImageOrgElement 	 = gOrgElement..Image
         ELSE
         	IMG_ImageOrgElement..Visible = False
         	nXOffshift is int = IMG_ImageOrgElement..Width + 6 // margin	
         	// retrieve the space for the caption
         	STC_TitleOrgElement..X			  = STC_TitleOrgElement..XInitial- nXOffshift
         	STC_TitleOrgElement..Width 	  = STC_TitleOrgElement..Width+ nXOffshift
         	STC_SubTitleOrgElement..X		  = STC_SubTitleOrgElement..XInitial- nXOffshift
         	STC_SubTitleOrgElement..Width = STC_SubTitleOrgElement..Width + nXOffshift
         END
         
         
