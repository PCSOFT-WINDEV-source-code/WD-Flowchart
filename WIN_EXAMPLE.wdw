                  sNameFlowChart is string
                  sFlowchart	 is string
                  nFlowchart	 is int
                  
                  // Asks for the name of the organization chart
                  //1: &Create
                  //2: C&ancel
                  IF Input(<§?1EC27638002A0B1B§>, sNameFlowChart) = 1 THEN
                  	
                  	// Defines the storage directory
                  	sFlowchart = OrgDirectory + ["\"] + sNameFlowChart
                  	
                  	// Creates the storage directory
                  	fMakeDir(sFlowchart)
                  	
                  	// Opens the organization chart
                  	IF OpenOrganizationChart(sFlowchart) THEN
                  		
                  		// Stores the organization chart
                  		nFlowchart = Add(FlowCharts, sFlowchart)
                  		
                  		// Adds the option to open it
                  		MenuAddOption(MENU_OPEN, "OPT_ORG" + nFlowchart, sNameFlowChart, OpenOrganizationChart, sFlowchart)
                  		ExecuteCode("OPT_ORG" + nFlowchart + "..Image = ""org_icon.png""")
                  	END
                  END
