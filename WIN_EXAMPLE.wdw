  procedures :
   -
     name : OpenOrganizationChart
     internal_properties : CAAAAAgAAAAM34km+p2LM0Jafr/gijXeTImHicFj7lFu9FrqtgEK91qgYssHbROYrXPWPQuCRnMMZ684xbe6oq1nscH9xKapdVr2FWTPC1MVk/+hO52w5+vn7IjZ6fSvrcAYUpDA0PhZ/QJLYO5TzRKT4VFwJvmoQ216mG+7u8SECyh/lCtC1H7uDeo=
     procedure_id : 1754141502131904708
     type_code : 14
     code : |1-
      // Summary: Opens an organization chart
      // Syntax:
      //[ <Result> = ] OpenOrganizationChart (<sOrganizationChart> is ANSI string)
      //
      // Parameters:
      //	sOrganizationChart (ANSI string): Path of the organization chart
      // Return value:
      // 	boolean: True if the organization chart was opened, False otherwise
      //
      // Automatic process of errors: run the CASE ERROR block
      //
      PROCEDURE OpenOrganizationChart(LOCAL sFlowchart is string)
      
      // Clears the organization chart
      OrgDeleteAll(ORGA_FLOWCHART)
      
      // Data location
      HChangeDir(Person, sFlowchart)
      HCreationIfNotFound(Person)
      
      // Displays the organization chart
      HExecuteQuery(QRY_Flowchart)
      
      AnimationPrepare(MyWindow)
      
      MyWindow..Title = "WD Flowchart [" + ExtractString(sFlowchart, lastRank, fSep) + "]"
      OrgDisplay(ORGA_FLOWCHART)	
      
      // Enables the creation
      BTN_ITM_NEW_ORGELEMENT..State = Active
      
      gOrgElement = Null
      GR_SELECTION..State = Grayed
      
      AnimationPlay(animFadeIn, 15)
      
      RESULT True
      
      CASE Error:
      RESULT False
