               -
                 internal_properties : CAAAAAgAAAAtB0sfkV88pn/8n3q8h/g5+02MPChtExMHqjuYKKXv93E0lmBbePnNGDOztFBepU6fqgoAEBS2WKrR9MlEjHrYMTXIH1kIjPxQko5YWl50fm/dPGcu95o7PFwpQFs98xjJuatlRv1rUe1wkco=
                 code : |1-
                  
                  nID 			is int = gOrgElement..ID
                  nManagerID	is int
                  
                  // Finds the form
                  HReadSeekFirst(Person, PersonID, nID)
                  IF NOT HFound(Person) THEN Error("Unable to delete")
                  
                  //1: &Delete
                  //2: Do&n't delete
                  IF Dialog(<§?1EC28C090073439A§>, gOrgElement..Title) = 1 THEN
                  	
                  	// No manager
                  	IF Person.ManagerID = 0 THEN
                  		
                  		// Delete without check (the deletion will assign a default manager ot the subordinates (ie: none))
                  		HDelete(Person)
                  	ELSE
                  		
                  		// Move up in the hierarchy
                  		HReadSeekFirst(Person, PersonID, Person.ManagerID)
                  		IF HFound(Person) THEN
                  			
                  			// No higher manager
                  			IF Person.ManagerID = 0 THEN
                  				
                  				// Delete without check (the deletion will assign a default manager ot the subordinates (ie: none))
                  				HReadSeekFirst(Person, PersonID, nID)
                  				HDelete(Person)
                  			ELSE
                  				
                  				// Stores the identifier of the higher manager
                  				nManagerID = Person.ManagerID			
                  				
                  				// Modify the manager of subordinates
                  				FOR EACH Person WHERE ManagerID = nID
                  					Person.ManagerID = nManagerID
                  					HModify(Person)
                  				END
                  				
                  				// Re-selects the person to delete
                  				HReadSeekFirst(Person, PersonID, nID)
                  				HDelete(Person)
                  			END
                  		END
                  	END
                  	
                  	// Displays the organization chart
                  	HExecuteQuery(QRY_Flowchart)
                  	OrgDisplay(ORGA_FLOWCHART)	
                  	
                  	AnimationPrepare(MyWindow)
                  	
                  	// Cancels the reference
                  	gOrgElement = Null
                  	GR_SELECTION..State = Grayed
                  	AnimationPlay(animFadeIn, 15)
                  END
                 type : 18
